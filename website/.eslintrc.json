{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    /* "eslint:recommended",  "next/core-web-vitals"*/
  ],
  "plugins": ["react"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "react/no-unescaped-entities": "error"
  }
}
